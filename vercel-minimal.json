{
  "builds": [
    {
      "src": "api/minimal.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9",
        "pip_requirements": "requirements-minimal.txt"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/minimal.py"
    }
  ],
  "env": {
    "VERCEL": "1"
  }
}
